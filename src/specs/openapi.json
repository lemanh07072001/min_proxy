{
  "openapi": "3.1.0",
  "info": {
    "title": "Get Api Proxy",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://app.mktproxy.com"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/api/proxies/new": {
      "get": {
        "tags": [
          "Api Get Proxy"
        ],
        "summary": "Lấy proxy xoay",
        "description": "API trả về proxy HTTP/SOCKS5 với key xác thực qua query parameter.\n\nVí dụ: `https://app.mktproxy.com/api/proxies/new?key=11765339914y0hoin`",
        "parameters": [
          {
            "name": "key",
            "in": "query",
            "required": true,
            "description": "Key xác thực proxy",
            "schema": {
              "type": "string",
              "example": "11765339914y0hoin"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Thành công",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "realIpAddress": {
                          "type": "string",
                          "example": "192.168.1.100"
                        },
                        "http": {
                          "type": "string",
                          "example": "192.168.1.100:8080:proxyuser123:secretPass456"
                        },
                        "httpPort": {
                          "type": "string",
                          "example": "8080"
                        },
                        "host": {
                          "type": "string",
                          "example": "192.168.1.100"
                        },
                        "user": {
                          "type": "string",
                          "example": "proxyuser123"
                        },
                        "pass": {
                          "type": "string",
                          "example": "secretPass456"
                        },
                        "timeRemaining": {
                          "type": "integer",
                          "example": 60
                        },
                        "message": {
                          "type": "string",
                          "example": "Proxy hiện tại, có thể xoay sau 60s"
                        }
                      }
                    },
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "code": {
                      "type": "integer",
                      "example": 200
                    },
                    "status": {
                      "type": "string",
                      "example": "SUCCESS"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Lỗi xác thực",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Unauthorized"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Lỗi server",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Internal Server Error"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
